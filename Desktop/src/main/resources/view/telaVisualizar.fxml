<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="610.0" prefWidth="1250.0" stylesheets="@css/styles.css" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ideau.ControlePatrimonioDesktop.controller.VisualizarController">
<VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
   <children>
      <HBox prefHeight="44.0" prefWidth="1250.0" styleClass="hBoxCadastro">
         <children>
            <VBox prefHeight="90" spacing="15.0" HBox.hgrow="ALWAYS">
               <children>
                  <Button fx:id="btnPatrimonios" mnemonicParsing="false" onAction="#abrirVisualizarPatrimonio" onMouseClicked="#abrirCadastroPatrimonios" prefHeight="40.0" prefWidth="150.0" text="Patrimonios">
                        <font>
                           <Font size="14.0" />
                        </font></Button>
               </children>
            </VBox>
            <VBox prefHeight="90.0" spacing="15.0" HBox.hgrow="ALWAYS">
               <children>
                  <Button fx:id="btnNotas" mnemonicParsing="false" onAction="#abrirVisualizarNotas" onMouseClicked="#abrirCadastroUsuarios" prefHeight="40.0" prefWidth="150.0" text="Notas Fiscais">
                        <font>
                           <Font size="14.0" />
                        </font>
                     </Button>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </VBox>
            <VBox prefHeight="90" spacing="15.0" HBox.hgrow="ALWAYS">
               <children>
                  <Button fx:id="btnFornecedores" mnemonicParsing="false" onAction="#abrirVisualizarFornecedores" onMouseClicked="#abrirCadastroFornecedores" prefHeight="40.0" prefWidth="150.0" text="Fornecedores">
                        <font>
                           <Font size="14.0" />
                        </font></Button>
               </children>
            </VBox>
               <VBox prefHeight="90.0" spacing="15.0">
                  <children>
                     <Button fx:id="btnUsuarios" mnemonicParsing="false" onAction="#abrirVisualizarUsuario" onMouseClicked="#abrirCadastroUsuarios" prefHeight="40.0" prefWidth="150.0" text="Usuarios">
                        <font>
                           <Font size="14.0" />
                        </font>
                     </Button>
                  </children>
                  <opaqueInsets>
                     <Insets />
                  </opaqueInsets>
               </VBox>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </HBox>
   </children>
</VBox>
   <AnchorPane fx:id="container" prefHeight="200.0" prefWidth="200.0" AnchorPane.topAnchor="48.0">
      <children>
         <HBox alignment="CENTER" prefHeight="100.0" prefWidth="1066.0" AnchorPane.leftAnchor="92.0" AnchorPane.topAnchor="0.0">
            <children>
               <ImageView cache="true" fitHeight="620.0" fitWidth="908.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@icones-png/gestor2.png" />
                  </image>
                  <HBox.margin>
