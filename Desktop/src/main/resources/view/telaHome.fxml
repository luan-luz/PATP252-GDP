<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane style="@css/styles.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ideau.ControlePatrimonioDesktop.controller.DashboardController">
<VBox>
   <children>
      <HBox alignment="CENTER" spacing="120.0">
         <children>
               <VBox>
                  <children>
                     <Button fx:id="btnQtdSetores" mnemonicParsing="false" prefHeight="60.0" prefWidth="150.0" styleClass="btnDashboard" text="Setores">
                        <graphic>
                        <ImageView fitHeight="24.0" fitWidth="24" preserveRatio="true">
                           <image>
                              <Image url="@icones-png/icone-relatorio-branco.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     </Button>
                     <Label text="Label" />
                  </children>
               </VBox>
            <VBox maxHeight="Infinity" maxWidth="Infinity" VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="btnQtdItens" mnemonicParsing="false" prefHeight="60.0" prefWidth="150.0" styleClass="btnDashboard" text="Itens">
                     <graphic>
                        <ImageView fitHeight="24.0" fitWidth="24" preserveRatio="true">
                           <image>
                              <Image url="@icones-png/icone-relatorio-branco.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Label fx:id="lblQtdItens" text="Label" />
               </children>
            </VBox>
            <VBox maxHeight="Infinity" maxWidth="Infinity" VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="btnQtdPatrimonio" mnemonicParsing="false" prefHeight="60.0" prefWidth="150.0" styleClass="btnDashboard" text="Patrimonio">
                     <graphic>
                        <ImageView fitHeight="24.0" fitWidth="24" preserveRatio="true">
                           <image>
                              <Image url="@icones-png/icone-relatorio-branco.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Label fx:id="lblQtdPatirmonio" text="Label" />
               </children>
            </VBox>
            <VBox maxHeight="Infinity" maxWidth="Infinity" VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="btnValorTotal" mnemonicParsing="false" prefHeight="60.0" prefWidth="150.0" styleClass="btnDashboard" text="Vlr_Patrimonio">
                     <graphic>
                        <ImageView fitHeight="24.0" fitWidth="24" preserveRatio="true">
                           <image>
                              <Image url="@icones-png/icone-relatorio-branco.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Label fx:id="lblValorTotal" text="Label" />
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
         </padding>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER" spacing="60.0">
            <children>
               <TableView fx:id="tablePrecoMedio" prefHeight="500.0" prefWidth="600.0">
                  <columns>
                     <TableColumn fx:id="colCategoria" prefWidth="200" text="Categoria" />
                     <TableColumn fx:id="colItem" prefWidth="250" text="Item" />
                     <TableColumn fx:id="colPrecoMedio" prefWidth="150" text="Preço Médio" />
                  </columns>
               </TableView>
               <VBox spacing="9.0">
                  <children>
                     <Label text="Categorias" />
                     <ListView fx:id="ListCategorias" prefHeight="120.0" prefWidth="420.0" />
                     <Label text="Quantidade de item por status" />
                     <ListView fx:id="ListStatus" prefHeight="120.0" prefWidth="450.0" />
                  </children>
               </VBox>
            </children>
            <padding>
               <Insets bottom="30.0" left="30.0" right="30.0" top="15.0" />
            </padding>
      </HBox>
   </children>
</VBox>
</AnchorPane>
